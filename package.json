{
  "name": "prisma-presentation",
  "description": "A app to show off features of prisma connected to a postgresql database",
  "private": true,
  "keywords": ["prisma", "postgresql"],
  "author": "Marcus Hellams",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "prebuild": "rimraf build",
    "build:bundle": "node scripts/build.js",
    "prebuild:bundle": "yarn build  && node scripts/rename.js",
    "postbuild:bundle": "rimraf build1",
    "start": "ts-node src/index.ts",
    "dev": "tsc-watch --onSuccess \"ts-node src/index.ts\"",
    "dev:debug": "tsc-watch --onSuccess \"node --inspect -r ts-node/register src/index.ts\"",
    "start:prod": "node build/",
    "prestart:prod": "npm run build",
    "format": "prettier . --write --ignore-path .gitignore && git update-index --again",
    "lint": "eslint . --config .eslintrc.build.json --ext .ts,.tsx --fix --ignore-path .gitignore",
    "test": "vitest --reporter=verbose",
    "test:ci": "vitest run --reporter=verbose",
    "test:coverage": "vitest --watch=false --silent --coverage --reporter=verbose",
    "prepare": "husky install",
    "prisma:int": "prisma init",
    "prisma:generate": "prisma generate",
    "seed:users": "ts-node seeding/user.ts",
    "seed:roles": "ts-node seeding/role.ts",
    "seed:books": "ts-node seeding/book.ts",
    "seed": "run-s seed:*",
    "studio": "prisma studio",
    "deleteAll": "ts-node utils/deleteAll.ts"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.7.8",
    "@types/papaparse": "^5.3.7",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitest/coverage-c8": "^0.22.1",
    "commander": "^10.0.0",
    "esbuild": "^0.15.5",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.11.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vitest": "^0.22.1"
  },
  "dependencies": {
    "@ngneat/falso": "^6.4.0",
    "@prisma/client": "^4.11.0",
    "fs-extra": "^11.1.0",
    "inquirer": "8.2.2",
    "papaparse": "^5.4.0"
  }
}
